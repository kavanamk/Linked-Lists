class AllOne(object):

    def __init__(self):
        dict={}
        

    def inc(self, key):
        """
        :type key: str
        :rtype: None
        """
        if dict[key]:
            dict[key]+=1
        else:
            dict[key]=1
        

    def dec(self, key):
        """
        :type key: str
        :rtype: None
        """
        if dict[key]==1:
            del dict[key]
        else:
            dict[key]-=1
        

    def getMaxKey(self):
        """
        :rtype: str
        """
        maxVal=0
        maxKey=''
        for key,val in dict.values():
            if val>maxVal:
                maxVal=val
                maxKey=key
        return maxKey

        

    def getMinKey(self):
        """
        :rtype: str
        """
        minVal=999999
        minKey=''
        for key,val in dict.values():
            if val<maxVal:
                minVal=val
                minKey=key
        return minKey
        


# Your AllOne object will be instantiated and called as such:
# obj = AllOne()
# obj.inc(key)
# obj.dec(key)
# param_3 = obj.getMaxKey()
# param_4 = obj.getMinKey()
